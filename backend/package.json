{
  "name": "creatorcoin-backend",
  "version": "1.0.0",
  "description": "Backend API for CreatorCoin AI platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback && knex migrate:latest && knex seed:run"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "knex": "^2.5.1",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "@solana/web3.js": "^1.78.5",
    "@solana/spl-token": "^0.3.8",
    "@coral-xyz/anchor": "^0.28.0",
    "axios": "^1.5.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "ffmpeg": "^0.0.4",
    "ws": "^8.13.0",
    "socket.io": "^4.7.2",
    "bull": "^4.11.3",
    "node-cron": "^3.0.2",
    "uuid": "^9.0.0",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.4",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2"
  }
}
