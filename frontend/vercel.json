{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/",
      "destination": "/web-demo.html"
    },
    {
      "source": "/demo",
      "destination": "/web-demo.html"
    },
    {
      "source": "/lynx",
      "destination": "/index.html"
    },
    {
      "source": "/index.lynx.bundle",
      "destination": "/index.lynx.bundle"
    },
    {
      "source": "/index.web.bundle",
      "destination": "/index.web.bundle"
    },
    {
      "source": "/(.*)",
      "destination": "/web-demo.html"
    }
  ],
  "headers": [
    {
      "source": "/index.lynx.bundle",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/index.web.bundle",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
