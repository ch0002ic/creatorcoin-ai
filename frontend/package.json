  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.4"
  },
  "scripts": {
    "dev": "rspeedy dev",
    "build": "rspeedy build && npm run build:html",
    "build:html": "echo '<!DOCTYPE html><html lang=\"en\"><head><meta charset=\"UTF-8\" /><meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" /><title>CreatorCoin AI - TikTok TechJam 2025</title><style>body{margin:0;font-family:-apple-system,BlinkMacSystemFont,\"Segoe UI\",\"Roboto\",\"Oxygen\",\"Ubuntu\",\"Cantarell\",\"Open Sans\",\"Helvetica Neue\",sans-serif;background:linear-gradient(135deg,#667eea 0%,#764ba2 100%);min-height:100vh}#root{width:100%;min-height:100vh}</style></head><body><div id=\"root\"></div><script type=\"module\" src=\"/index.web.bundle\"></script></body></html>' > dist/index.html",
    "vercel-build": "npm run build",
    "preview": "rspeedy preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "type-check": "tsc --noEmit",
    "ios": "rspeedy build --target lynx",
    "android": "rspeedy build --target lynx",
    "web": "rspeedy dev --target web",
    "build:production": "NODE_ENV=production rspeedy build",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "deploy:netlify": "npm run build:production && netlify deploy --prod --dir=dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
